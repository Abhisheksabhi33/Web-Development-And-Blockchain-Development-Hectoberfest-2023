    <style>
    *{
      margin: 0;
      padding: 0;
      box-sizing: border-box;
      font-family: 'Sora', sans-serif;
      }
      html{
        height: 100%;
      }
      /* questions */
    .individualQuestionsContainer::-webkit-scrollbar-track
    {
      border: 1px solid black;
      background-color: #F5F5F5;
    }

    .individualQuestionsContainer::-webkit-scrollbar
    {
      width: 8px;
      background-color: #F5F5F5;
    }

    .individualQuestionsContainer::-webkit-scrollbar-thumb
    {
      background-color: #555151;	
    }

    /* status bar */
    .statusBar::-webkit-scrollbar-track
    {
      border: 1px solid black;
      background-color: #F5F5F5;
    }

    .statusBar::-webkit-scrollbar
    {
      width: 5px;
      background-color: #F5F5F5;
    }

    .statusBar::-webkit-scrollbar-thumb
    {
      background-color: #555151;	
    }

    
    /* body  */
    body::-webkit-scrollbar-track
    {
      border: 1px solid black;
      background-color: #F5F5F5;
    }

    body::-webkit-scrollbar
    {
      width: 10px;
      background-color: #F5F5F5;
    }

    body::-webkit-scrollbar-thumb
    {
      background-color: #555151;	
    }
    body{
      height: 100vh;
      display: flex;
      flex-direction: column;

      background: #000000;  /* fallback for old browsers */
      background: -webkit-linear-gradient(to top, #434343, #000000);  /* Chrome 10-25, Safari 5.1-6 */
      background: linear-gradient(to top, #969494c2, #f7e4e4); /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */

      background-repeat: no-repeat;
      background-attachment: fixed;
      margin: 0;
    }

    footer {
      padding: 3px;
      background-color:  rgb(52 58 64);
      color: white;
      margin-top: auto;
     
    }
    a{
      cursor: pointer;
    }
